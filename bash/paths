#!/bin/bash

export PATH=$HOME/bin/jruby/bin:$PATH
export PATH="~/.rbenv/shims:~/.rbenv/bin:/usr/local/bin:/usr/local/sbin:/usr/local/share/npm/bin:/usr/local/mysql/bin:~/dotfiles/bin:~/Workspace/taco/bin:~/Workspace/littlelines-suspenders/bin:/usr/local/pgsql/bin:/usr/bin:  $PATH"
export MANPATH="/usr/local/man:/usr/local/mysql/man:/usr/local/git/man:$MANPATH"
export PATH=/opt/local/bin:/opt/local/sbin:$PATH
export MAGICK_HOME=/usr/local/include/ImageMagick
export PATH=$MAGICK_HOME/bin:$PATH
export NODE_PATH="/usr/local/lib/node_modules:$NODE_PATH"
export JRUBY_OPTS="-Xcext.enabled=true"

export EMACS="/usr/local/Cellar/emacs/25.2-rc2/bin/emacs"
export PATH=$PATH:/usr/local/opt/go/libexec/bin
export PATH=$HOME/.cask/bin:$PATH
export PATH="./bin:$PATH"
export PATH="$(brew --prefix qt@5.5)/bin:$PATH"
